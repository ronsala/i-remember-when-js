<h1><%= @user.username %>'s Memories</h1>
<table>
  <thead>
    <th>Event</th><th>Date</th><th>Title</th><th>Added</th>
  </thead>
    <% @user.memories.each do |memory| %>
      <tr>
        <td><%= link_to memory.event.name, event_path(memory.event) %></td>
        <td><%= format_date(memory.event.date) %></td>
        <td><%= link_to memory.title, memory_path(memory) %></td>
        <td><%= format_date(memory.created_at) %></td>
      </tr>
    <% end %>
</table>
